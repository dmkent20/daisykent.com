<% for category in Category.all %>
<div id="<%= category.id %>" class="">
  <%= check_box_tag "video[category_ids][]", category.id, @video.categories.include?(category), {:id => dom_id(category, dom_id(@video)) } %>
  <%= category.name %>
  <%= link_to "x", category, remote: true, method: :delete, data: {confirm: "Are you sure? Deleting this tags will permanently remove it from all posts."}  %>
</div>
<% end %>
<div id="newcat" class=""></div>
